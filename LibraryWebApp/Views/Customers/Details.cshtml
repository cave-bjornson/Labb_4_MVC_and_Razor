@model LibraryWebApp.Views.Customers.CustomerViewModel

@{
    ViewBag.Title = "Customer Details";
}

<abp-card abp-border="Dark">
    <abp-card-header>@ViewBag.Title</abp-card-header>
    <abp-card-body>
        <abp-card-title>@Model.Name @Model.Surname</abp-card-title>
        <abp-card-text>With supporting text below as a natural lead-in to additional content.</abp-card-text>
        <a abp-button="Primary" asp-action="Index">Go Back</a>
    </abp-card-body>
</abp-card>